@model EntityLayer.Concrete.SocialMedia
@{
    ViewData["Title"] = "UpdateSocialMedia";
    Layout = "~/Views/Shared/_NewAdminLayout.cshtml";
}


<div class="card-header border-0">
    <h3>Sosyal Medya Güncelleme Sayfası</h3>
</div>
<br />
<br />
<form action="/SocialMedia/UpdateSocialMedia/" method="post">
    @Html.HiddenFor(x=>x.SocialMediaID)
    <label>Sosyal Medya</label>
    @Html.TextBoxFor(x=>x.Name,new{@class="form-control"})
    <br />
    <label>Profil Linki</label>
    @Html.TextBoxFor(x=>x.URL,new{@class="form-control"})
    <br />
    <label>İkon</label>
    @Html.TextBoxFor(x=>x.Icon,new{@class="form-control"})

    <br />
    <button class="btn btn-outline-primary">Kaydet</button>

</form>


